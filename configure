#----------------------------------------------------------------------------
# Host-dependent configuration
# $host: $host_cpu-$host_vendor-$host_os
#----------------------------------------------------------------------------

MYcxxcmp()
   {
     mycxx=$1
     shift

     while test $# -ge 1 ; do
       if ( echo "${mycxx}" | ${GREP} "$1\$" >/dev/null 2>&1 ); then
         return 0
       fi
       shift
     done
     return 1
   }

