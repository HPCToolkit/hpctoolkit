#-------------------------------------------------
# Host-specific defines
#-------------------------------------------------

# NOTE: To see what macros gcc-like compilers define:
#   gcc -dM -E - < /dev/null | sort
#
# FIXME: This should probably be split into separate CPU and OS
# sections.

HOST_OS=
HOST_CPU_MIPS=no
HOST_CPU_PPC=no
HOST_CPU_X86=no
HOST_CPU_X86_64=no
HOST_CPU_X86_FAMILY=no
HOST_CPU_IA64=no
HOST_CPU_AARCH64=no

case "${host}" in
  # Linux
  i386*-*-linux* | i686*-*-linux*)
    # __i386 && __linux
    HOST_OS="Linux"
    HOST_CPU_X86=yes
    HOST_CPU_X86_FAMILY=yes
    UNWINDER=x86-family

$as_echo "#define HOST_CPU_x86 1" >>confdefs.h


$as_echo "#define HOST_OS_LINUX 1" >>confdefs.h

    ;;

  x86_64*-*-linux*)
    # __x86_64 && __linux
    HOST_OS="Linux"
    if test "${host_sizeof_voidp}" = "4"; then
      HOST_CPU_X86=yes

$as_echo "#define HOST_CPU_x86 1" >>confdefs.h

    else
      HOST_CPU_X86_64=yes

$as_echo "#define HOST_CPU_x86_64 1" >>confdefs.h

    fi
    HOST_CPU_X86_FAMILY=yes
    UNWINDER=x86-family

$as_echo "#define HOST_OS_LINUX 1" >>confdefs.h

    ;;

  ia64*-*-linux*)
    # __ia64 && __linux
    HOST_OS="Linux"
    HOST_CPU_IA64=yes
    UNWINDER=libunw

$as_echo "#define HOST_CPU_IA64 1" >>confdefs.h

    $as_echo "#define HOST_OS_LINUX /**/" >>confdefs.h

    ;;

  mips64*-*-linux*)
    # __mips64 && __linux / _MIPS_SIM == _ABIN32 / _MIPS_SIM == _ABI64
    HOST_OS="Linux"
    HOST_CPU_MIPS=yes
    UNWINDER=mips

$as_echo "#define HOST_PLATFORM_MIPS64LE_LINUX 1" >>confdefs.h


$as_echo "#define HOST_OS_LINUX 1" >>confdefs.h

    ;;

  powerpc64*-*-linux* | powerpc-*-linux*)
    # []
    HOST_OS="Linux"
    HOST_CPU_PPC=yes
    UNWINDER=ppc64

$as_echo "#define HOST_OS_LINUX 1" >>confdefs.h


$as_echo "#define HOST_CPU_PPC 1" >>confdefs.h

    ;;

  # MacOS
  powerpc*-*-darwin*)
    # __ppc__ / __MACH__
    HOST_OS="MacOS"
    HOST_CPU_PPC=yes
    UNWINDER=ppc64

$as_echo "#define HOST_OS_MACOS 1" >>confdefs.h


$as_echo "#define HOST_CPU_PPC 1" >>confdefs.h

    ;;

  i386*-*-darwin* | i686*-*-darwin*)
    HOST_OS="MacOS"
    HOST_CPU_X86=yes
    UNWINDER=x86-family

$as_echo "#define HOST_CPU_x86 1" >>confdefs.h


$as_echo "#define HOST_OS_MACOS 1" >>confdefs.h

    ;;

  x86_64*-*-darwin*)
    HOST_OS="MacOS"
    if test "${host_sizeof_voidp}" = "4"; then
      HOST_CPU_X86=yes

$as_echo "#define HOST_CPU_x86 1" >>confdefs.h

    else
      HOST_CPU_X86_64=yes

$as_echo "#define HOST_CPU_x86_64 1" >>confdefs.h

    fi
    HOST_CPU_X86_FAMILY=yes
    UNWINDER=x86-family

$as_echo "#define HOST_OS_MACOS 1" >>confdefs.h

    ;;

  # IRIX
  mips*-*-irix*)
    # __mips && __sgi && __unix
    # _MIPS_ISA == _MIPS_ISA_MIPS1 | _MIPS_ISA == _MIPS_ISA_MIPS2
    # _MIPS_ISA == _MIPS_ISA_MIPS3 | _MIPS_ISA == _MIPS_ISA_MIPS4
    HOST_OS="IRIX"
    HOST_CPU_MIPS=yes
    UNWINDER=mips

$as_echo "#define HOST_OS_IRIX 1" >>confdefs.h

    ;;

  # Solaris
  sparc*-*-solaris*)
    # __sparc && __sun && __unix
    HOST_OS="Solaris"

$as_echo "#define HOST_OS_SOLARIS 1" >>confdefs.h

    ;;

  # Tru64/OSF
  alpha*-*-osf*)
    # __alpha / __digital__ && __unix__
    HOST_OS="Tru64"

$as_echo "#define HOST_OS_TRU64 1" >>confdefs.h

    ;;

  # ARM 64-bit
  aarch64*linux* )
    HOST_OS="Linux"
    HOST_CPU_AARCH64=yes
    UNWINDER=libunw

$as_echo "#define HOST_OS_LINUX 1" >>confdefs.h


$as_echo "#define HOST_CPU_ARM64 1" >>confdefs.h

    ;;

  *)
    as_fn_error $? "HPCToolkit is not configured for HOST=${host}!" "$LINENO" 5
    ;;
esac

HOST_CPU_K1OM=no
case "$host" in
  *k1om* ) HOST_CPU_K1OM=yes ;;
esac

