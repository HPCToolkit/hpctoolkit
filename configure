  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: unable to find openmp library" >&5
$as_echo "$as_me: WARNING: unable to find openmp library" >&2;}
fi

openmp_lib_dir=

if test "$openmp_lib_name" != no ; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for openmp library directory" >&5
$as_echo_n "checking for openmp library directory... " >&6; }

  name=`$CXX -print-file-name="$openmp_lib_name" 2>/dev/null`
  if test ! -f "$name" ; then
    name=`ldd "$obj" 2>/dev/null | grep -e "$openmp_lib_name" | $AWK '{print $3}'`
  fi

  if test -f "$name" ; then
    dir=`dirname "$name"`
    if test -d "$dir" ; then
      openmp_lib_dir="$dir"
    fi
  fi

  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $openmp_lib_dir" >&5
$as_echo "$openmp_lib_dir" >&6; }

  if test ! -d "$openmp_lib_dir" ; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: unable to find openmp library directory" >&5
$as_echo "$as_me: WARNING: unable to find openmp library directory" >&2;}
  fi
fi

rm -f "$src" "$obj"


#-------------------------------------------------
# LD -- find correct arch type for ld
#-------------------------------------------------

# Autoconf gets the wrong arch type for ld in a cross compile, at
# least on Intel MIC.  Here, we test that ld supports relocatable
# linking (ld -r).  If not, then try other arch types until we find
# one that works.

test_ld_r()
{
  ld_cmd="$*"
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking relocatable linking: $ld_cmd " >&5
$as_echo_n "checking relocatable linking: $ld_cmd ... " >&6; }
  rm -f "$obj3"
  $ld_cmd -r "$obj1" "$obj2" -o "$obj3" >&5 2>&1
  if test $? -eq 0 && test -f "$obj3" ; then
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
    return 0
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
    return 1
  fi
}

src1="conftest.$$.1.c"
src2="conftest.$$.2.c"
obj1="conftest.$$.1.o"
obj2="conftest.$$.2.o"
obj3="conftest.$$.3.o"

rm -f "$src1" "$src2" "$obj1" "$obj2" "$obj3"

echo 'int foo(void) { return 2; }' >"$src1"
echo 'int baz(void) { return 4; }' >"$src2"

$CC $CFLAGS -c "$src1" -o "$obj1" >&5 2>&1
$CC $CFLAGS -c "$src2" -o "$obj2" >&5 2>&1

found_ld=no

if test_ld_r $LD ; then
  # autoconf's choice for LD works
  found_ld=yes
else
  # try other arch types from LD -V
  # be careful not to create a subshell (redirect fd)
  ld_base=`echo $LD | awk '{ print $1 }'`

  emul=false
  arch_list=`$ld_base -V 2>&1`
  for arch in $arch_list
  do
    if $emul && test_ld_r $ld_base -m $arch ; then
      LD="$ld_base -m $arch"
      found_ld=yes
      break
    fi
    # skip output until 'Supported emulations:'
    case "$arch" in
      *emulat* ) emul=true ;;
    esac
  done
fi

rm -f "$src1" "$src2" "$obj1" "$obj2" "$obj3"

if test "$found_ld" != yes ; then
  as_fn_error $? "unable to find correct arch type for ld" "$LINENO" 5
fi


#-------------------------------------------------
# MPICXX compiler for hpcprof-mpi
#-------------------------------------------------

# Search for MPI C++ compiler that is on PATH but not from a module
# and is not a spack wrapper.  This is mainly for a spack build on
# Cray or Blue Gene where mpicxx is not from a module that we can put
# in packages.yaml.
#
# On Cray, we must avoid the spack wrapper.  The Cray MPI C++ compiler
# is CC, but the spack CC wrapper is only a C++ compiler, not MPI.
#
#  $1, $2, ... is a list of compiler names (per platform).
#
# Returns: full path of compiler, or else 'no'.
#
search_for_mpi()
{
  ans=no

  for name in "$@" ; do
    for item in `type -a $name 2>/dev/null` ; do
      case "$item" in
        /* )
          # spack wrapper: /path/to/spack/lib/spack/env/case-insensitive/CC
          echo "$item" | grep -E -i -e 'lib.*spack.*env' >/dev/null
          if test $? -ne 0 ; then
            ans="$item"
            break 2
          fi
          ;;
      esac
    done
  done

  echo "$ans"
}

#-------------------------------------------------

# Find MPICXX, the MPI C++ compiler for hpcprof-mpi.
#
# --enable-mpi-search=(cray, bgq) is mainly for a spack build on Cray
# or Blue Gene that doesn't have a module for MPI.  Outside of spack,
# set MPICXX and use --enable-all-static for Cray and Blue Gene that
# build a static hpcprof-mpi.



{ $as_echo "$as_me:${as_lineno-$LINENO}: supplied MPICXX compiler: $MPICXX" >&5
$as_echo "$as_me: supplied MPICXX compiler: $MPICXX" >&6;}

# Check whether --enable-mpi-search was given.
if test "${enable_mpi_search+set}" = set; then :
  enableval=$enable_mpi_search;
else
  enable_mpi_search=no
fi


case "$enable_mpi_search" in
  cray )
    MPICXX=`search_for_mpi CC`
    case "$MPICXX" in
      /* )
        { $as_echo "$as_me:${as_lineno-$LINENO}: Cray MPI wrapper: $MPICXX" >&5
$as_echo "$as_me: Cray MPI wrapper: $MPICXX" >&6;}
	;;
      * )
        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: unable to find the Cray MPI C++ wrapper (CC) on your PATH" >&5
$as_echo "$as_me: WARNING: unable to find the Cray MPI C++ wrapper (CC) on your PATH" >&2;}
        as_fn_error $? "check that this system is Cray and CC is the MPI wrapper" "$LINENO" 5
        ;;
    esac
    ;;

  bgq )
    MPICXX=`search_for_mpi mpicxx mpic++`
    case "$MPICXX" in
      /* )
        { $as_echo "$as_me:${as_lineno-$LINENO}: Blue Gene MPI wrapper: $MPICXX" >&5
$as_echo "$as_me: Blue Gene MPI wrapper: $MPICXX" >&6;}
	;;
      * )
        { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: unable to find the Blue Gene MPI C++ wrapper (mpicxx) on your PATH" >&5
$as_echo "$as_me: WARNING: unable to find the Blue Gene MPI C++ wrapper (mpicxx) on your PATH" >&2;}
        as_fn_error $? "check that this system is Blue Gene and mpicxx is the MPI wrapper" "$LINENO" 5
        ;;
    esac
    ;;

  no )
    ;;

  * )
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --enable-mpi-search is for Spack builds on Cray and Blue Gene" >&5
$as_echo "$as_me: WARNING: --enable-mpi-search is for Spack builds on Cray and Blue Gene" >&2;}
    as_fn_error $? "unknown platform: $enable_mpi_search" "$LINENO" 5
    ;;
esac

#-------------------------------------------------

# Test if MPICXX is a valid MPI C++ compiler and report version.

if test "x$MPICXX" != x ; then
  MPICXX_VERSION=`find_version "$MPICXX"`
  MPICXX_PATH=`find_path "$MPICXX"`
  MPICXX="$MPICXX_PATH"

  { $as_echo "$as_me:${as_lineno-$LINENO}: MPICXX compiler: $MPICXX_VERSION" >&5
$as_echo "$as_me: MPICXX compiler: $MPICXX_VERSION" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}: MPICXX path: $MPICXX_PATH" >&5
$as_echo "$as_me: MPICXX path: $MPICXX_PATH" >&6;}

  if test_is_gnu "$MPICXX" ; then : ; else
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Not using the GNU MPICXX compiler." >&5
$as_echo "$as_me: WARNING: Not using the GNU MPICXX compiler." >&2;}
    warn_non_gnu=yes
    warn_non_gnu_mesg="$warn_non_gnu_mesg MPICXX"
  fi

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking if MPICXX is a valid MPI C++ compiler" >&5
$as_echo_n "checking if MPICXX is a valid MPI C++ compiler... " >&6; }

  ORIG_CXX="$CXX"
  CXX="$MPICXX"
  ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu


  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */


#include <mpi.h>
#include <iostream>
int myRank(void)
{
  int rank;
  MPI_Comm_rank(MPI_COMM_WORLD, &rank);
  std::cout << "yes";
  return rank;
}

_ACEOF
if ac_fn_cxx_try_compile "$LINENO"; then :
  mpicxx_avail=yes
else
  mpicxx_avail=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext

  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $mpicxx_avail" >&5
$as_echo "$mpicxx_avail" >&6; }

  if test "$mpicxx_avail" = no ; then
    as_fn_error $? "MPICXX is not a valid compiler: $MPICXX" "$LINENO" 5
  fi

  ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
