      as_fn_error $? "invalid boost directory: $BOOST" "$LINENO" 5
    fi
    BOOST_IFLAGS="-I${BOOST}/include"

    for lib in $multilib_path ; do
      dir="${BOOST}/$lib"
      for f in "$dir"/libboost*system*.so* ; do
        if test -f "$f" ; then
	  BOOST_LIB_DIR="$dir"
	  BOOST_COPY=yes
	  break 2
	fi
      done
    done
    if test "$BOOST_COPY" != yes ; then
      as_fn_error $? "invalid boost directory: $BOOST" "$LINENO" 5
    fi
    num_pkgs=`expr 1 + $num_pkgs`
    ;;
  no )
    ;;
  * )
    as_fn_error $? "boost directory must be absolute path: $BOOST" "$LINENO" 5
    ;;
esac

