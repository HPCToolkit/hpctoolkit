#-------------------------------------------------
# Utilities for compiler versions and paths
#-------------------------------------------------

# These tests only use GNU syntax.  Intel, Clang, etc use a different
# format, but we don't really build with those.
# For example:  gcc (GCC) 7.3.1 20180130 (Red Hat 7.3.1-2)

find_version()
{
  ans='unknown non-gnu'

  # strip any compiler flags, 'gcc -m64'
  base=`echo $1 | awk '{ print $1 }'`
  version=`$base --version 2>/dev/null`
  echo "$version" | grep -i copyright >/dev/null
  if test $? -eq 0 ; then
    ans=`echo "$version" | head -1`
  fi

  echo "$ans"
}

find_path()
{
  ans=
  base=`echo $1 | awk '{ print $1 }'`
  case "$base" in
    /* ) ans="$base" ;;
    * )  # Extract the first word of "$base", so it can be a program name with args.
set dummy $base; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_ans+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $ans in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_ans="$ans" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_ans="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  test -z "$ac_cv_path_ans" && ac_cv_path_ans="no"
  ;;
esac
fi
ans=$ac_cv_path_ans
if test -n "$ans"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ans" >&5
$as_echo "$ans" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi

 ;;
  esac
  if test "x$ans" = xno ; then
    ans=unknown
  fi

  echo "$ans"
}

test_is_gnu()
{
  base=`echo $1 | awk '{ print $1 }'`
  version=`$base --version 2>/dev/null`
  echo $version | grep -E -i -e 'copy.*free.*soft.*found' >/dev/null

  return $?
}


#-------------------------------------------------
# C, C++ compiler versions and paths
#-------------------------------------------------

# Find path and version for CC/CXX, test if GNU, and check if recent
# enough version >= 5.x.

cc_is_gnu=yes

CC_VERSION=`find_version "$CC"`
CC_PATH=`find_path "$CC"`

{ $as_echo "$as_me:${as_lineno-$LINENO}: C compiler: $CC_VERSION" >&5
$as_echo "$as_me: C compiler: $CC_VERSION" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}: C path: $CC_PATH" >&5
$as_echo "$as_me: C path: $CC_PATH" >&6;}

if test_is_gnu "$CC" ; then : ; else
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Not using the GNU C compiler." >&5
$as_echo "$as_me: WARNING: Not using the GNU C compiler." >&2;}
  cc_is_gnu=no
fi

CXX_VERSION=`find_version "$CXX"`
CXX_PATH=`find_path "$CXX"`

{ $as_echo "$as_me:${as_lineno-$LINENO}: C++ compiler: $CXX_VERSION" >&5
$as_echo "$as_me: C++ compiler: $CXX_VERSION" >&6;}
{ $as_echo "$as_me:${as_lineno-$LINENO}: C++ path: $CXX_PATH" >&5
$as_echo "$as_me: C++ path: $CXX_PATH" >&6;}

if test_is_gnu "$CXX" ; then : ; else
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Not using the GNU C++ compiler." >&5
$as_echo "$as_me: WARNING: Not using the GNU C++ compiler." >&2;}
  cc_is_gnu=no
fi

warn_non_gnu=no
warn_non_gnu_mesg=

warn_old_gnu=no

warn_identify_compiler=no
warn_identify_compiler_mesg=
have_cc_version=no

# parse CC version: gcc (GCC) 7.3.1 20180130 (Red Hat 7.3.1-2)
if test "$cc_is_gnu" = yes ; then
  ver=`echo $CC_VERSION | sed -e 's/^[^)]*[)]//' | awk '{print $1}'`
  cc_major=`echo $ver | awk -F. '{print $1}'`
  cc_minor=`echo $ver | awk -F. '{print $2}'`

  if test "x$cc_minor" != x ; then
    have_cc_version=yes
    if test "$cc_major" -lt 5 ; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: CC/CXX compiler too old for hpctoolkit" >&5
$as_echo "$as_me: WARNING: CC/CXX compiler too old for hpctoolkit" >&2;}
      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Use GNU version 5.x or later" >&5
$as_echo "$as_me: WARNING: Use GNU version 5.x or later" >&2;}
      warn_old_gnu=yes
    fi
  else
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: unable to identify CC/CXX compiler version: $CC_VERSION" >&5
$as_echo "$as_me: WARNING: unable to identify CC/CXX compiler version: $CC_VERSION" >&2;}
    warn_identify_compiler=yes
    warn_identify_compiler_mesg='CC/CXX'
  fi
else
  warn_non_gnu=yes
  warn_non_gnu_mesg='CC/CXX'
fi


#-------------------------------------------------
# Find base CC compiler, outside of spack
#-------------------------------------------------

# Hpclink needs the CC compiler for the fnbounds array, but the spack
# wrapper won't run outside of spack.  So, if we're being built by
# spack, then find the base compiler for hpclink.

HPCLINK_CC="$CC_PATH"

echo "$CC_PATH" | grep -E -i -e 'lib.*spack.*env' >/dev/null
if test $? -eq 0 ; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: found spack compiler wrapper" >&5
$as_echo "$as_me: found spack compiler wrapper" >&6;}
  { $as_echo "$as_me:${as_lineno-$LINENO}: checking if base compiler works: $SPACK_CC" >&5
$as_echo_n "checking if base compiler works: $SPACK_CC... " >&6; }

  ORIG_CC="$CC"
  CC="$SPACK_CC"
  ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu


  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */


unsigned long addrs[] = {
  0x400400, 0x400410
};
unsigned long len = sizeof(addrs) / sizeof(addrs[0]);

_ACEOF
if ac_fn_c_try_compile "$LINENO"; then :
  ans=yes
else
  ans=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext

  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $ans" >&5
$as_echo "$ans" >&6; }

  ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu

  CC="$ORIG_CC"

  if test "$ans" = yes ; then HPCLINK_CC="$SPACK_CC"
  else HPCLINK_CC=gcc
  fi
fi

{ $as_echo "$as_me:${as_lineno-$LINENO}: hpclink compiler: $HPCLINK_CC" >&5
$as_echo "$as_me: hpclink compiler: $HPCLINK_CC" >&6;}




#-------------------------------------------------
# CXXFLAGS for std C++11
#-------------------------------------------------

# Find a CXX flag for standard C++11 and add to CXXFLAGS if needed.
# We use some C++11 features (auto) in the parseAPI code in
# src/lib/banal.  Unfortunately, due to include files, we pretty much
# need this everywhere for C++ code.



c11_list='argvar none -std=c++11 -std=c++0x -std=gnu++0x'
cxx_c11_avail=no
cxx_c11_flag=

ORIG_CXXFLAGS="$CXXFLAGS"
ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for CXX flag for C++11" >&5
$as_echo_n "checking for CXX flag for C++11... " >&6; }

for opt in $c11_list
do
  case "$opt" in
    argvar ) flag="$CXX11_FLAG" ;;
    none )   flag=  ;;
    * )      flag="$opt" ;;
  esac
  CXXFLAGS="$HOST_CXXFLAGS $ORIG_CXXFLAGS $flag"
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */


#include <list>
int mylen(std::list <int> lst)
{
  int ans = 0;
  for (auto it = lst.begin(); it != lst.end(); it++) {
    ans += 1;
  }
  return ans;
}

_ACEOF
if ac_fn_cxx_try_compile "$LINENO"; then :
  ans=yes
else
  ans=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
  if test "$ans" = yes ; then
    cxx_c11_avail=yes
    cxx_c11_flag="$flag"
    break
  fi
done

{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $cxx_c11_flag" >&5
$as_echo "$cxx_c11_flag" >&6; }

ac_ext=cpp
ac_cpp='$CXXCPP $CPPFLAGS'
ac_compile='$CXX -c $CXXFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CXX -o conftest$ac_exeext $CXXFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_cxx_compiler_gnu


if test "$cxx_c11_avail" = no ; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: unable to find CXXFLAGS for std C++11" >&5
$as_echo "$as_me: WARNING: unable to find CXXFLAGS for std C++11" >&2;}
  as_fn_error $? "set the CXX11_FLAG variable and rerun configure" "$LINENO" 5
fi

CXXFLAGS="$ORIG_CXXFLAGS"
HOST_CXXFLAGS="$HOST_CXXFLAGS $cxx_c11_flag"



#-------------------------------------------------

# Find location of libstdc++.  This is used to set LD_LIBRARY_PATH in
# hpcfnbounds.  It is not used by hpcfnbounds2.
#
# Try --print-file-name first.  If that doesn't work, then try ldd.
# If --print-file-name works, then its answer is more reliable because
# it doesn't depend on LD_LIBRARY_PATH.  But if you use CC= without
# setting LD_LIBRARY_PATH, then ldd will give the wrong answer.
#
# Of course, --print-file-name and libstdc++.so are GNU-specific.
# (Intel icc seems to support both, but computes an answer based on
# LD_LIBRARY_PATH, which can't be right.)  Is there a better option
# here?

libcxx_file=`${CXX} -print-file-name=libstdc++.so`

case "$libcxx_file" in
  /* ) ;;
  * )
    src="hpc_hello.$$.cc"
    obj="hpc_hello.$$.out"
    rm -f "$src" "$obj"
    cat <<"EOF" >"$src"
main(){}
EOF
    ${CXX} -o "$obj" "$src"
    libcxx_file=`ldd "$obj" | $AWK '/libstdc\+\+/ { print $3 }'`
    rm -f "$src" "$obj"
    ;;
esac

libcxx_dir=`dirname $libcxx_file`
libcxx_base=`basename $libcxx_file`
HPCRUN_LIBCXX_PATH=`( cd $libcxx_dir 2>/dev/null && /bin/pwd )`

case "$HPCRUN_LIBCXX_PATH" in
  /* ) { $as_echo "$as_me:${as_lineno-$LINENO}: found libstdc++.so at ${HPCRUN_LIBCXX_PATH}/${libcxx_base}" >&5
$as_echo "$as_me: found libstdc++.so at ${HPCRUN_LIBCXX_PATH}/${libcxx_base}" >&6;} ;;
  * )  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: unable to find libstdc++.so" >&5
$as_echo "$as_me: WARNING: unable to find libstdc++.so" >&2;} ;;
esac




