$as_echo_n "checking if binutils version 2.34 or later... " >&6; }

cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */


#define PACKAGE "hpctoolkit"
#include <bfd.h>
unsigned long
get_vma(asection * asec)
{
    return bfd_section_vma(asec);
}

_ACEOF
