#-------------------------------------------------
# Hash value for launch scripts
#-------------------------------------------------

# Make a hash value to verify that a launch script has the correct
# install directory.  It doesn't have to be secure, just unique.

for ac_prog in md5sum sha256sum sha384sum sha512sum
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_hash_fcn+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $hash_fcn in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_hash_fcn="$hash_fcn" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_hash_fcn="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
hash_fcn=$ac_cv_path_hash_fcn
if test -n "$hash_fcn"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $hash_fcn" >&5
$as_echo "$hash_fcn" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


  test -n "$hash_fcn" && break
done
test -n "$hash_fcn" || hash_fcn="no"


plain_text=`echo $$ ; date ; pwd`

hash_value=`echo $plain_text | $hash_fcn | $AWK '{ print $1 }'`

if test "x$hash_value" = x ; then
  hash_value=`echo $plain_text | tr -d ' '`
fi

hash_value=`echo $hash_value | head -c 30`

{ $as_echo "$as_me:${as_lineno-$LINENO}: launch script hash: $hash_value" >&5
$as_echo "$as_me: launch script hash: $hash_value" >&6;}




