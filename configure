#-------------------------------------------------

# Find the openmp library and its directory.  We add the directory to
# the launch script for hpcstruct when using openmp.

openmp_lib_list='libgomp libiomp libomp libxlsmp'

src="conftest.$$.cpp"
obj="conftest.$$.bin"

rm -f "$src" "$obj"

cat <<EOF >"$src"
#include <iostream>
#include <omp.h>
int main(int argc, char **argv)
{
  int i;
#pragma omp parallel for
  for (i = 0; i < argc; i++) {
    std::cout << i << "\n";
  }
}
EOF

openmp_lib_name=no

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for openmp library" >&5
$as_echo_n "checking for openmp library... " >&6; }

$CXX -g $CXXFLAGS $openmp_flag "$src" -o "$obj" >/dev/null 2>&1

if test $? -eq 0 && test -f "$obj" ; then
  for lib in $openmp_lib_list ; do
    base=`ldd "$obj" 2>/dev/null | $AWK '{print $1}' | grep -e "$lib"`
    if test "x$base" != x ; then
      openmp_lib_name="$base"
      break
    fi
  done
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: unable to compile openmp with $openmp_flag" >&5
$as_echo "$as_me: WARNING: unable to compile openmp with $openmp_flag" >&2;}
fi

{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $openmp_lib_name" >&5
$as_echo "$openmp_lib_name" >&6; }

