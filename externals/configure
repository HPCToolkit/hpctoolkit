# Check that CC and CXX produce the same wordsize.  Also check that
# CFLAGS doesn't change the wordsize.  This shouldn't be necessary,
# but some packages can't handle -m64 in CFLAGS, they require
# something like CC='gcc -m64'.

srcfile="conftest.$$.c"
objfile="conftest.$$.o"

rm -f "$srcfile"
cat >"$srcfile" <<EOF
int foo(void)
{ return 2; }
EOF

get_wordsize()
{
    output=`file $objfile`
    case "$output" in
	*64-bit* ) ans=64 ;;
	*32-bit* ) ans=32 ;;
	* ) ans=0 ;;
    esac
    echo $ans
}

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking host wordsize from CC" >&5
$as_echo_n "checking host wordsize from CC... " >&6; }
rm -f "$objfile"
$CC -c -o "$objfile" "$srcfile" >/dev/null
wordsize_cc=`get_wordsize`
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $wordsize_cc bits" >&5
$as_echo "$wordsize_cc bits" >&6; }

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking host wordsize from CXX" >&5
$as_echo_n "checking host wordsize from CXX... " >&6; }
rm -f "$objfile"
$CXX -c -o "$objfile" "$srcfile" >/dev/null
wordsize_cxx=`get_wordsize`
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $wordsize_cxx bits" >&5
$as_echo "$wordsize_cxx bits" >&6; }

rm -f "$objfile"
$CC $CFLAGS -c -o "$objfile" "$srcfile" >/dev/null
wordsize_cflags=`get_wordsize`

rm -f "$objfile"
$CXX $CXXFLAGS -c -o "$objfile" "$srcfile" >/dev/null
wordsize_cxxflags=`get_wordsize`

rm -f "$srcfile" "$objfile"

if test "$wordsize" != "$wordsize_cc" \
    || test "$wordsize" != "$wordsize_cxx" \
    || test "$wordsize" != "$wordsize_cflags" \
    || test "$wordsize" != "$wordsize_cxxflags"
then
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: CC or CXX generate inconsistent wordsizes." >&5
$as_echo "$as_me: WARNING: CC or CXX generate inconsistent wordsizes." >&2;}
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Either CC ($wordsize_cc) and CXX ($wordsize_cxx) generate different wordsizes," >&5
$as_echo "$as_me: WARNING: Either CC ($wordsize_cc) and CXX ($wordsize_cxx) generate different wordsizes," >&2;}
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: or they do not match the host wordsize ($wordsize), or they generate" >&5
$as_echo "$as_me: WARNING: or they do not match the host wordsize ($wordsize), or they generate" >&2;}
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: different wordsizes with and without CFLAGS.  If you are" >&5
$as_echo "$as_me: WARNING: different wordsizes with and without CFLAGS.  If you are" >&2;}
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: using a CFLAGS option to reset the wordsize, then put this" >&5
$as_echo "$as_me: WARNING: using a CFLAGS option to reset the wordsize, then put this" >&2;}
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: option in CC with something like CC='gcc -m${wordsize}'." >&5
$as_echo "$as_me: WARNING: option in CC with something like CC='gcc -m${wordsize}'." >&2;}
    as_fn_error $? "CC or CXX generate inconsistent wordsizes." "$LINENO" 5
fi

#------------------------------------------------------------
# Copy skeleton directories
#------------------------------------------------------------

# Support a separate build directory by copying the package skeleton
# directories.  Just copy regular files and the patches subdir, don't
# copy other subdirs.

packages='binutils boost libdwarf libelf libmonitor libunwind libxml2
old-monitor open-analysis oprofile symtabAPI xed2 xerces'

if test "x$srcdir" != x.
then
    { $as_echo "$as_me:${as_lineno-$LINENO}: checking copying package directories" >&5
$as_echo_n "checking copying package directories... " >&6; }
    for pkg in $packages build-utils
    do
        if mkdir -p "$pkg" ; then : ; else
	    as_fn_error $? "unable to mkdir $pkg" "$LINENO" 5
	fi
	for file in "${srcdir}/${pkg}"/*
	do
	    if test -f "$file" ; then
		if cp -f "$file" "$pkg" ; then : ; else
		    as_fn_error $? "unable to copy $file" "$LINENO" 5
		fi
	    fi
	done
	for dir in "${srcdir}/${pkg}"/patch*
	do
	    if test -d "$dir" ; then
		base=`basename "$dir"`
		if mkdir -p "${pkg}/${base}" ; then : ; else
		    as_fn_error $? "unable to mkdir $pkg/$base" "$LINENO" 5
		fi
		for file in "$dir"/*
		do
		    if test -f "$file" ; then
			if cp -f "$file" "${pkg}/${base}" ; then : ; else
			    as_fn_error $? "unable to copy $file" "$LINENO" 5
			fi
		    fi
		done
	    fi
	done
    done

    # Symlink the distfiles directory.
    if test ! -d distfiles ; then
	if ln -s "${srcdir}/distfiles" distfiles ; then : ; else
	    as_fn_error $? "unable to symlink distfiles" "$LINENO" 5
	fi
    fi

    { $as_echo "$as_me:${as_lineno-$LINENO}: result: done" >&5
$as_echo "done" >&6; }
fi

