#!/bin/sh
#
#  SymtabAPI 8.2 build.
#
#  $Id$
#

PKG_NAME=symtabAPI
src=dyninst

subdirs='elf dwarf common symtabAPI'

. ../build-utils/configure.inc
pre_build

cd work || die "unable to cd: work"
work_dir=`pwd`

for dir in $subdirs ; do
    echo "building $dir ..."
    cd "$work_dir/$dir" || die "unable to cd: $work_dir/$dir"
    $MAKE || die "unable to build $dir"
done

post_build
