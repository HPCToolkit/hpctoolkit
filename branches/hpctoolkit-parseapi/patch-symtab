     -DCMAKE_CXX_COMPILER="$symtab_cxx_path" \
     -DPATH_BOOST="$boost_prefix" \
-    -DIBERTY_LIBRARY="$binutils_prefix/lib/libiberty.a" \
+    -DIBERTY_LIBRARIES="$binutils_prefix/lib/libiberty.a" \
     -DLIBELF_INCLUDE_DIR="$libelf_inc" \
