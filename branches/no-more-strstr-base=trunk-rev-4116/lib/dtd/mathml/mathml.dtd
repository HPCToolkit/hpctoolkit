<!ENTITY % att-xmlns
                                     'xmlns%mathml-colon;%mathml-prefix;
                                      CDATA #FIXED 
                                     "http://www.w3.org/1998/Math/MathML"' >
<!ENTITY  % att-global
  '%att-xmlns;
   xmlns:%mathml-xlink;               CDATA #IMPLIED
